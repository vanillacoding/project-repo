{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": "warn",
    "eol-last": "warn",
    "no-multi-spaces": "warn",
    "no-undef": "off",
    "no-trailing-spaces": "warn",
    "quotes": ["warn", "double"],
    "comma-style": ["warn", "last"],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
