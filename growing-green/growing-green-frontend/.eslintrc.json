{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "prettier", "plugin:cypress/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "no-nested-ternary": 0,
    "no-console": 1
  }
}
