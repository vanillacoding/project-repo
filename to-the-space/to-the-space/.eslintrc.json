{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "error",
    "no-param-reassign": ["error"],
    "ignorePatterns": ["**/node_modules", "**/dist"]
  }
}
